{
  "name": "gitke",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node src/index.js",
    "api": "apidoc -i src/ -o doc/",
    "dev": "DEBUG=git:* nodemon src/index.js"
  },
  "keywords": [
    "git"
  ],
  "author": "Kenny Wong <wowohoo@qq.com>",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "doc/*",
      ".gitke-data/*"
    ]
  },
  "apidoc": {
    "name": "Gitke",
    "version": "1.0.0",
    "description": "Simple git server",
    "apidoc": {
      "title": "Simple git server",
      "url": "https://api.github.com/v1"
    }
  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "dayjs": "^1.7.5",
    "fs-extra": "^7.0.0",
    "git-side-band-message": "0.0.3",
    "koa": "^2.5.2",
    "koa-body": "^4.0.4",
    "koa-bodyparser": "^4.2.1",
    "koa-generic-session": "^2.0.1",
    "koa-generic-session-sequelize": "^2.0.3",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "nodegit": "^0.22.2",
    "os-homedir": "^1.0.2",
    "sequelize": "^4.38.0",
    "sqlite3": "^4.0.2",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "nodemon": "^1.18.3"
  }
}
